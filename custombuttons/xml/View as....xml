                      id="cb-mimeTypesPopup"
                      context="toolbar-context-menu"
                      onpopupshowing="this.parentNode.populateMimeTypesMenu(event);"
                      oncommand="OIB_BrowserOverlay.onSetMimeCommand(event)">
           </menupopup>;

this.appendChild(custombuttonsUtils.makeXML(menu));
this.type = "menu";



]]></initcode>
