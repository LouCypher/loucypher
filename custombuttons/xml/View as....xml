  <initcode><![CDATA[if (typeof OpenInBrowser != "object") {
  this.tooltipText = "Open in Browser extension is not installed or is disabled.";
  this.style.opacity = ".25";
  this.disabled = true;
  return;
}

this.populateMimeTypesMenu = function cbOIBBO_populateViewAsMenu(aEvent) {
  var popup = aEvent.target;
  var uri = Application.activeWindow.activeTab.uri;
  OpenInBrowser.populateMenu(document.getElementById(aEvent.target.id), uri);
}

var menu = <menupopup xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"
