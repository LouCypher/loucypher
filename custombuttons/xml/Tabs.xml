  <mode>0</mode>
  <initcode><![CDATA[this.tabList = function(node) {
  var tabs = gBrowser.mTabs;
  for(var j = 0; j < tabs.length; j++) {
    mi = document.createElement("menuitem");
    mi.setAttribute("label", tabs[j].label);
    if(tabs[j].selected) mi.style.fontWeight = "bold";
    mi.setAttribute("oncommand", "gBrowser.selectedTab = gBrowser.mTabs[" + j + "]");
    mi.className = "menuitem-iconic";
    mi.setAttribute("image", tabs[j].image
                    ? tabs[j].image
                    : "chrome://mozapps/skin/places/defaultFavicon.png");
    mi.tooltipText = tabs[j].linkedBrowser.currentURI.spec;
    mi.index = j;
    mi.setAttribute("onclick", "this.parentNode.parentNode.closeTab(event);");
    node.appendChild(mi);
  }
}

this.removeList = function(node) {
  var list = node.childNodes.length;
  for(var i = 0; i < list; i++) {
    node.removeChild(node.lastChild);
  }
}

this.closeTab = function(e) {
  if (e.button != 1) return;
  gBrowser.closeTab(gBrowser.mTabs[e.target.index]);
  closeMenus(e.target);
}

var mPopup = document.createElement("menupopup");
mPopup.popupShowing = this.tabList.bind(this);
mPopup.popupHiding = this.removeList.bind(this);
mPopup.setAttribute("onpopupshowing", "this.popupShowing(this)");
mPopup.setAttribute("onpopuphiding", "this.popupHiding(this)");


this.appendChild(mPopup);
this.type = "menu";



this.setAttribute('homepage', 'http://web.archive.org/web/20100711122604/http://custombuttons2.com/en-us/forum/buttons/buttons-database/tabs-list-button-menu.html');

]]></initcode>
