  <initcode><![CDATA[this.reloadCSS = function reloadCSS() {
  var e = content.document.getElementsByTagName("link");
  for (var i = 0, n; n = e[i]; i++) {
    if (n.rel.toLowerCase().indexOf("stylesheet") >= 0 && n.href) {
      var h = n.href.replace(/[?&]cssReloader=([^&$]*)/, "");
      n.href = h + (h.indexOf("?") >= 0 ? "&" : "?") +
               "cssReloader=" + (new Date).valueOf();
    }
  }
}

