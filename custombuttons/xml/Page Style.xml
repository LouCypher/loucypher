  <initcode><![CDATA[var mp = <menupopup xmlns={xulns}
                    onpopupshowing="stylesheetFillPopup(this);"
                    oncommand="stylesheetSwitchAll(window.content,
 event.target.getAttribute('data')); setStyleDisabled(false);">
           <menuitem label="No Style" accesskey="n"
                     type="radio" checked="false"
                     oncommand="setStyleDisabled(true); event.stopPropagation();"/>
           <menuitem label="Basic Page Style" accesskey="b"
                     type="radio" checked="true" />
           <menuseparator hidden="true" />
         </menupopup>;

this.appendChild(cbu.makeXML(mp));

this.type = "menu";

]]></initcode>
