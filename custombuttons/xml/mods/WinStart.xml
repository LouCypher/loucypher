  <name>Start</name>
  <image><![CDATA[data:image/png;base64,]]></image>
  <mode>0</mode>
  <initcode><![CDATA[/*Initialization Code*/]]></initcode>
  <code><![CDATA[var vbs = "Option Explicit\r\n\
Dim WshShell\r\n\
Set WshShell = WScript.CreateObject(\"WScript.Shell\")\r\n\
WshShell.SendKeys(\"^{ESC}\")\r\n\
";

var file = Components.classes["@mozilla.org/file/local;1"]
                     .createInstance(Components.interfaces.nsILocalFile);
file.initWithPath("C:\\");
file.append("start.vbs");

if (!file.exists()) {
  custombuttons.writeFile(file.path, vbs);
}

file.launch();
]]></code>
