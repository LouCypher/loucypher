<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Mozilla Themes</os:ShortName>
<os:Description>Lightweight Themes</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/png;base64,</os:Image>
<SearchForm>https://addons.mozilla.org/themes/</SearchForm>
<os:Url type="text/html" method="GET" template="https://addons.mozilla.org/search/">
  <os:Param name="q" value="{searchTerms}"/>
  <os:Param name="cat" value="themes"/>
</os:Url>
</SearchPlugin>
